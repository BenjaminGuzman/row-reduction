{
  "name": "row-reduction",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tailwind": "tailwindcss build --input src/styles.scss --config tailwind.config.js --output src/tailwind.css",
    "start": "ng serve --configuration=development & npm run tailwind -- --watch",
    "build": "npm run tailwind && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run row-reduction:serve-ssr",
    "serve:ssr": "node dist/row-reduction/server/main.js",
    "build:ssr": "ng build && ng run row-reduction:server",
    "prerender": "ng run row-reduction:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.14",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.14",
    "@angular/compiler": "^12.2.14",
    "@angular/core": "^12.2.14",
    "@angular/forms": "^12.2.14",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.14",
    "@angular/platform-browser-dynamic": "^12.2.14",
    "@angular/platform-server": "^12.2.14",
    "@angular/router": "^12.2.14",
    "@nguniversal/express-engine": "^12.1.3",
    "express": "^4.15.2",
    "fraction.js": "^4.1.2",
    "katex": "^0.13.24",
    "rxjs": "~6.6.0",
    "tailwindcss": "^3.0.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.13",
    "@angular/cli": "^12.2.13",
    "@angular/compiler-cli": "^12.2.14",
    "@nguniversal/builders": "^12.1.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/katex": "^0.11.1",
    "@types/node": "^12.20.37",
    "jasmine-core": "~3.8.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}